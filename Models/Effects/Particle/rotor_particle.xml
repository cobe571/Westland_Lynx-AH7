<?xml version="1.0" encoding="UTF-8"?><PropertyList><!-- ==                                   == --><!-- == ROTOR PARTICLE SYSTEMS ANIMATIONS == --><!-- ==                                   == --><!-- dry particles -->	<model>		<name>dirt_01</name>		<path>Aircraft/Westland_Lynx-AH7/Models/Effects/particle/rotor_particle_dry_H.xml</path>		<offsets>			<x-m>  2.973 </x-m>			<y-m>  0.000 </y-m>			<z-m> -2.500 </z-m>		</offsets>	</model>		<model>		<name>dirt_02</name>		<path>Aircraft/Westland_Lynx-AH7/Models/Effects/particle/rotor_particle_dry_L.xml</path>		<offsets>			<x-m>  2.973 </x-m>			<y-m>  0.000 </y-m>			<z-m> -1.900 </z-m>		</offsets>	</model>	<!-- wet particles -->	<model>		<name>dirt_03</name>		<path>Aircraft/Westland_Lynx-AH7/Models/Effects/particle/rotor_particle_wet_H.xml</path>		<offsets>			<x-m>  2.973 </x-m>			<y-m>  0.000 </y-m>			<z-m> -1.500 </z-m>		</offsets>	</model>		<model>		<name>dirt_04</name>		<path>Aircraft/Westland_Lynx-AH7/Models/Effects/particle/rotor_particle_wet_L.xml</path>		<offsets>			<x-m>  2.973 </x-m>			<y-m>  0.000 </y-m>			<z-m> -0.900 </z-m>		</offsets>	</model>				<animation>		<name>dirt</name>			<object-name>dirt_01</object-name>			<object-name>dirt_02</object-name>			<object-name>dirt_03</object-name>			<object-name>dirt_04</object-name>	</animation>	<!-- move particle system with pich and roll -->	<animation>		<type>rotate</type>		<object-name>dirt</object-name>		<!-- <property>orientation/pitch-deg</property>  -->		<property>sim/multiplay/generic/float[6]</property>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>rotate</type>		<object-name>dirt</object-name>		<!-- <property>orientation/roll-deg</property> -->		<property>sim/multiplay/generic/float[8]</property>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>translate</type>		<object-name>dirt</object-name>			<!--			<property>position/gear-agl-m</property>			-->			<property>sim/multiplay/generic/float[7]</property>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>	<!-- move particle system with rotor tilt -->	<animation>		<object-name>dirt</object-name>               <property>rotors/main/roll-deg</property>		<factor>1</factor>		<center>			<x-m> 2.973</x-m>			<y-m> 0.000</y-m>			<z-m> 1.751</z-m>		</center>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>			<animation>		<object-name>dirt</object-name>               <property>rotors/main/yaw-deg</property>		<factor>1</factor>		<center>			<x-m> 2.973</x-m>			<y-m> 0.000</y-m>			<z-m> 1.751</z-m>		</center>		<axis>			<x>0</x>			<y>1</y>			<z>0</z>		</axis>	</animation>	</PropertyList>