<PropertyList>

<path>instr_ffkgh.ac</path>

	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		
		<emission>
			        <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/dash-dim</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<object-name>shadow</object-name>
		
		<emission>
		  
			        <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/dash-dim</factor-prop>
		</emission>
	</animation>	

	<animation>
		<type>noshadow</type>
		<object-name>shadow</object-name>
		
	</animation>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>shadow</object-name>
  </effect>

<animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>/engines/engine[0]/fuel-flow_pph</property>
  <interpolation>
    <entry><ind>    0.00</ind><dep>   0.0</dep></entry>
    <entry><ind> 2204.62</ind><dep>  28.0</dep></entry>
    <entry><ind>13227.73</ind><dep> 277.3</dep></entry>
    <entry><ind>22046.21</ind><dep> 338.0</dep></entry>
    <entry><ind>50000</ind><dep>    338.0</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>
