<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>vsi-gaz.ac</path>
	
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
  </animation>

 <animation>
<object-name>glass</object-name>
</animation>

<animation>
<type>noshadow</type>
<object-name>glass</object-name>
</animation>

<animation>
<type>shader</type>
<shader>chrome</shader>
<texture>Aircraft/Generic/Effects/glass_shader.png</texture>
<object-name>glass</object-name>
</animation> 


  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle1</object-name>
 <emission>
            <red>   0.005 </red>
      <green> 0.015 </green>
      <blue>  0.030 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>
	
  <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
   <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
 <!--	<property>sim/multiplay/generic/float[19]</property>-->
    <interpolation>
     <entry><ind> -2500 </ind><dep>  -165 </dep></entry>
     <entry><ind> -2000 </ind><dep>  -142 </dep></entry>
      <entry><ind> -1500 </ind><dep> -110 </dep></entry>
      <entry><ind> -1000 </ind><dep>  -82 </dep></entry>
      <entry><ind>  -500 </ind><dep>  -40 </dep></entry>
      <entry><ind>     0 </ind><dep>    0 </dep></entry>
      <entry><ind>   500 </ind><dep>   40 </dep></entry>
      <entry><ind>  1000 </ind><dep>   82 </dep></entry>
      <entry><ind>  1500 </ind><dep>  110 </dep></entry>
      <entry><ind>  2000 </ind><dep>  142 </dep></entry>
      <entry><ind>  2500 </ind><dep>  165 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

