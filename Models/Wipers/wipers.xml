<PropertyList>

  <path>wipers.ac</path>

<!-- wiper animation -->
	
	<animation>
	<name>wiper_cplt</name>
	<object-name>wiperarm.001</object-name>
	<object-name>wiperblade.001</object-name>
	</animation>
	
	<animation>
	<name>wiper_plt</name>
	<object-name>wiperarm.002</object-name>
